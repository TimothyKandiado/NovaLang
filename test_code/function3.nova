fn sqrt(value)
    return value^(1/2)
end

fn vec_distance(x, y)
    sqr_distance := x^2 + y^2
    distance := sqrt(sqr_distance)
    return distance
end

fn main()
    length := 10
    width := 5

    dist := vec_distance

    hypotenuse := dist(length, width)

    println("Distance = ", hypotenuse)
end

main()

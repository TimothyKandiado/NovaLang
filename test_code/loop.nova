fn main()
    iterations := 1E6
    index := 0
    type := "milli"

    println(iterations, " iterations")

    start_time := time(type)

    while index < iterations
        index = index + 1
    end

    end_time := time(type)
    println("Elapsed time = ", end_time - start_time, " ", type)
end

println("Looping benchmark")
main()
